{
  "name": "api-solid",
  "version": "1.0.0",
  "description": "Gympass style app",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --poll --respawn --transpile-only src/server.ts",
    "start": "node build/server.js",
    "build": "tsup src --out-dir build",
    "test": "vitest run --dir src/use-cases",
    "test:create-prisma-envinroment": "npm link ./prisma/vitest-environment-prisma",
    "test:install-prisma-envinroment": "npm link vitest-environment-prisma",
    "test:watch": "vitest --dir src/use-cases",
    "pretest:e2e": "run-s test:create-prisma-envinroment test:install-prisma-envinroment",
    "test:e2e": "vitest run --dir src/http",
    "test:e2e:watch": "vitest --dir src/http",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "9.5.0",
    "@rocketseat/eslint-config": "2.2.2",
    "@types/bcryptjs": "2.4.6",
    "@types/node": "^20.14.8",
    "@types/supertest": "6.0.2",
    "@vitest/coverage-v8": "2.0.1",
    "eslint": "9.5.0",
    "globals": "15.6.0",
    "npm-run-all": "4.1.5",
    "prisma": "5.16.0",
    "supertest": "7.0.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsup": "^8.1.0",
    "typescript": "^5.5.2",
    "typescript-eslint": "8.0.0",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "2.0.1"
  },
  "dependencies": {
    "@fastify/cookie": "9.3.1",
    "@fastify/jwt": "8.0.1",
    "@prisma/client": "5.16.0",
    "bcryptjs": "2.4.3",
    "dayjs": "1.11.12",
    "dotenv": "16.4.5",
    "fastify": "^4.28.0",
    "zod": "3.23.8"
  }
}
